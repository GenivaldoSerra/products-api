{
  "name": "products-api-backend",
  "version": "1.0.0",
  "description": "Backend API para gerenciamento de produtos",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "migrate": "prisma migrate dev",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:rollback": "prisma migrate reset",
    "build": "echo 'Build completed'",
    "test": "jest"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "@prisma/client": "^5.7.1",
    "zod": "^3.22.4",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@jest/globals": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/server.js"
    ]
  },
  "keywords": ["api", "products", "nodejs", "express", "postgresql"],
  "author": "",
  "license": "MIT"
}
